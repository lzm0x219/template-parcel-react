{
  "name": "template-parcel-react",
  "version": "1.0.0",
  "description": "ðŸ“¦ A minimal React Parcel starter template.",
  "type": "module",
  "source": "index.html",
  "scripts": {
    "build": "tsc --noEmit && parcel build",
    "build:report": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky install",
    "preview": "http-server dist --port 4901 -S --cert ./certs/localhost.cert --key ./certs/localhost.key",
    "serve": "parcel --port 4900 --cert ./certs/localhost.cert --key ./certs/localhost.key",
    "start": "pnpm serve"
  },
  "lint-staged": {
    "*.{js,cjs,ts,tsx,md,html,css,json,yml}": "prettier --write",
    "!(*pnpm-lock).yaml": "prettier --write"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3"
  },
  "devDependencies": {
    "@parcel/config-default": "2.8.0",
    "@parcel/core": "2.8.0",
    "@parcel/reporter-bundle-analyzer": "2.8.0",
    "@parcel/reporter-bundle-buddy": "2.8.0",
    "@parcel/transformer-svg-react": "2.8.0",
    "@parcel/validator-typescript": "2.8.0",
    "@types/parcel-env": "0.0.1",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "http-server": "14.1.1",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.3",
    "parcel": "2.8.0",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.3.0",
    "prettier-plugin-tailwindcss": "0.1.13",
    "process": "0.11.10",
    "sort-package-json": "2.1.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    },
    "pseudoClasses": {
      "focusVisible": "focus-visible"
    }
  },
  "alias": {
    "@/*": "./src/$1"
  }
}
